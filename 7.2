div fos-terminal-prompt nu-prompt {
  background: none;
  pointer-events: none;
}
nu-prompt .prompt.with-header {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    height: 33%;
    width: 100%;
    pointer-events: auto;
}
.header.ng-star-inserted {
  background-color: #CC3B29;
  border: 1px solid #8B0000;
}
.console-controls button.bare:hover {
  background-color: black;
}
.minimized-content-tabs .content-tab {
  background-color: #CC3B29;
  border: 1px solid #8B0000;
}
.minimized-content-tabs button.bare:hover {
  background-color: black;
}
