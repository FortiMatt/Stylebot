.content-container.ng-star-inserted {
  height: 20vw;
  width: 100%;
}

.prompt.with-header {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
}

.explicit-bounds,.close-button.ng-star-inserted {
  background: none;
}

div.title.ng-star-inserted,.prompt.with-header {
  pointer-events: auto;
}

nu-prompt.explicit-bounds {
  pointer-events: none;
}

.ng-star-inserted {
  border: none;
}

.header .ng-star-inserted,nu-dockable-item-header.clickable {
  background-color: #CC3B29;
}

div.content-body.ng-star-inserted,.close-button.ng-star-inserted:hover,.minimize-button.ng-star-inserted:hover,.nu-button-allow-events.ng-star-inserted:hover,.nu-dockable-item-header.clickable {
  background-color: #000;
}
